{
  "$schema": "https://raw.githubusercontent.com/reVISit-studies/study/main/src/parser/StudyConfigSchema.json",
  "studyMetadata": {
    "title": "Color Association",
    "version": "pilot",
    "authors": [
      "The reVISit Team"
    ],
    "date": "2023-04-14",
    "description": "This study investigates how different colors prompt individuals to choose one option over another, focusing specifically on the selection of colored bins without explicit instructions. Our experiment is designed to understand the basic associations people make with colors in decision-making scenarios. Study baded on Color inference in visual communication: the meaning of colors in recycling by Karen B. Schloss et al., 2018, which looks at how color impacts trash and recycling behaviors.",
    "organizations": [
      "University of Utah",
      "WPI",
      "University of Toronto"
    ]
  },
  "uiConfig": {
    "contactEmail": "test@test.com",
    "helpTextPath": "can-demo/help.md",
    "logoPath": "assets/revisitLogoSquare.svg",
    "withProgressBar": true,
    "autoDownloadStudy": false,
    "sidebar": true
  },
